{
	"schemaVersion": 1,
	"id": "clipboard",
	"version": "${version}",
	"name": "Clipboard",
	"description": "A Minecraft mod that implements a clipboard manager into Minecraft, keeping history and letting you search through them!",
	"authors": [
		"Dooji"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/clipboard",
		"sources": "https://github.com/dooji2/clipboard"
	},
	"license": "GPL-v3",
	"icon": "assets/clipboard/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.dooji.clipboard.Clipboard"
		]
	},
	"mixins": [
		"clipboard.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"omnilib": ">=0.1.2"
	},
	"suggests": {
		"another-mod": "*"
	}
}